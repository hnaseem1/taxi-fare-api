function checkFields() {

    if (document.getElementById('field1').value && document.getElementById('field2').value){
        $.getJSON('/location/q=' + place.geometry.lat() + ',' + place.geometry
            .lng()).then(function (data) {
            window.localStorage.setItem('lalala', data);

        })

    }

}


autocomplete.addListener('place_changed', function onPlaceChange() {
    var place = autocomplete.getPlace();

    checkFields();
});

autocomplete2.addListener('place_changed', function onPlaceChange() {
    var place = autocomplete.getPlace();

    checkFields();
});
